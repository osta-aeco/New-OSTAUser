    <command:details>
      <command:name>New-OSTAUser</command:name>
      <maml:description>
        <maml:para>Creates a new AzureAD/Office 365 user for OSTA-AECO.</maml:para>
      </maml:description>
      <maml:copyright>
        <maml:para />
      </maml:copyright>
      <command:verb>New</command:verb>
      <command:noun>OSTAUser</command:noun>
      <dev:version />
    </command:details>
    <maml:description>
      <maml:para>The New-OSTAUser creates a new AzureAD/Office 365 user for OSTA-AECO. Compatible with both Windows PowerShell and PowerShell Core.</maml:para>
    </maml:description>
    <command:syntax>
